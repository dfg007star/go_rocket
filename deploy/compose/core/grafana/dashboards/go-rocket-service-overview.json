{
  "id": null,
  "title": "Go Rocket Service Overview",
  "description": "Основные метрики Go Rocket сервиса",
  "tags": ["go_rocket", "microservice"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Orders Total",
      "description": "Количество заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(orders_total[1m])",
          "legendFormat": "{{method}} - {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "title": "Orders Revenue Dashboard",
      "description": "Мониторинг выручки от заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "orders_revenue_total",
          "legendFormat": "Total Revenue",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "rate(orders_revenue_total[5m])",
          "legendFormat": "Revenue/sec (5m avg)",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "min": 0,
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Revenue/sec (5m avg)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD/s"
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "max", "mean"]
        }
      }
    },
    {
      "id": 3,
      "title": "Assembly Duration",
      "description": "Время выполнения сборки заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(assembly_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "datasource": {"type": "prometheus", "uid": "prometheus-uid"}
        },
        {
          "expr": "histogram_quantile(0.95, rate(assembly_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "datasource": {"type": "prometheus", "uid": "prometheus-uid"}
        },
        {
          "expr": "histogram_quantile(0.90, rate(assembly_duration_seconds_bucket[5m]))",
          "legendFormat": "p90",
          "datasource": {"type": "prometheus", "uid": "prometheus-uid"}
        },
        {
          "expr": "histogram_quantile(0.50, rate(assembly_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "datasource": {"type": "prometheus", "uid": "prometheus-uid"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "decimals": 3,
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "tooltip": {"mode": "multi", "sort": "desc"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    }
  ],
  "version": 1,
  "uid": "go-rocket-service-overview",
  "schemaVersion": 30
} 